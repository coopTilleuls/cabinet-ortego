{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tilleuls-ortego-blocks/hero-page",
	"version": "0.1.0",
	"title": "Hero Page",
	"category": "widgets",
	"icon": "smiley",
	"description": "Block hero por page secondaire",
	"supports": {
		"html": false,
		"align": ["full"],
		"innerBlocks": true
	},
	"textdomain": "hero-page",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js",
	"attributes": {
		"title": {
			"type": "string",
			"default": "Mon Titre"
		},
		"description": {
			"type": "string",
			"default": ""
		}
	}
}
