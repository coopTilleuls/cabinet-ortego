{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tilleuls-ortego-blocks/profile-block",
	"version": "0.1.0",
	"title": "Profil Avocat",
	"category": "design",
	"icon": "businessperson",
	"description": "Section de présentation avec photo, bio et compétences.",
	"supports": {
		"html": false,
		"align": ["wide", "full"]
	},
	"textdomain": "profile-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"title": {
			"type": "string",
			"source": "html",
			"selector": "h2",
			"default": "Engagement & Expertise"
		},
		"subtitle": {
			"type": "string",
			"source": "html",
			"selector": ".subtitle",
			"default": "Avocate au Barreau • Prestation de serment 2022"
		},
		"bio": {
			"type": "string",
			"source": "html",
			"selector": ".bio-content"
		},
		"imgId": {
			"type": "number",
			"default": 0
		},
		"imgUrl": {
			"type": "string",
			"default": ""
		},
		"languages": {
			"type": "string",
			"source": "html",
			"selector": ".languages-bar",
			"default": "<span class=\"lang-tag\">Français</span>"
		},
		"diplomas": {
			"type": "string",
			"source": "html",
			"selector": ".diploma-list",
			"default": "<div class=\"diploma-item\">Diplôme ici</div>"
		}
	}
}
